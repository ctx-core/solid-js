{
	"name": "@ctx-core/solid-js",
	"version": "1.5.217",
	"description": "ctx-core solid-js model helpers",
	"keywords": [
		"ctx-core",
		"solidjs",
		"pnpm"
	],
	"homepage": "https://github.com/ctx-core/nanostores#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/nanostores/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/nanostores.git"
	},
	"license": "Apache-2.0",
	"author": {
		"name": "Brian Takita",
		"url": "https://briantakita.me",
		"email": "info+ctx-core-solid-js@briantakita.me"
	},
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./_types",
		"./ctx__Context",
		"./params__ctx__memo_",
		"./signal__toggle_",
		"./subscribe",
		"./subscribe_wait"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test:unit",
		"test:unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"disable:test:unit:coverage": "c8 pnpm test:unit"
	},
	"dependencies": {
		"ctx-core": "*",
		"solid-js": "^1.9.10"
	},
	"devDependencies": {
		"@sinonjs/fake-timers": "^13.0.5",
		"@types/sinonjs__fake-timers": "^8.1.5",
		"c8": "^10.1.3",
		"check-dts": "^0.8.2",
		"eslint": "^9.39.1",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public"
	},
	"sideEffects": false
}